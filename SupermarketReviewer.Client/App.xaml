<Application x:Class="SupermarketReviewer.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Styles/Brushes.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/Consts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/Geometry.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/Texts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/Buttons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/Lists.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Styles/Window.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
